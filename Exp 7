using namespace std;
class circle{
   int cx,cy;
   float slope,circumference ,x1,x2,y1,y2;
   public:
   circle(int cx,int cy)
    {
       this->cx=cx;
       this->cy=cy;
    }
    circle(float x1, float x2, float y1, float y2)
    {
       this->x1=x1;
       this->x2=x2;
       this->y1=y1;
       this->y2=y2;
    }
    void show()
    {
       slope = (y2-y1)/(x2-x1);
       cout<<"slope is = "<<slope<<endl;
    }
     void getvalue()
     {
        cout<<"value fo x1,x2,y1,y2 ";
        cin>>x1>>x2>>y1>>y2;
     }
